{
  "name": "react-starter",
  "version": "0.0.5",
  "description": "All in one React Starter",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonybudianto/react-starter.git"
  },
  "scripts": {
    "start": "node .",
    "serve-dev": "node config/browsersync/browsersync.dev.js",
    "serve-build": "npm run build-prod && node config/browsersync/browsersync.prod.js",
    "watch": "npm run build-dev -- --watch",
    "build-dev": "webpack -d --config config/webpack/webpack.dev.config.js",
    "build-prod": "webpack -p --config config/webpack/webpack.prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "starter",
    "webpack",
    "browsersync"
  ],
  "author": "Antony Budianto <antonybudianto@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "node-sass": "^3.8.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.0"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^1.1.0"
  }
}
